; ModuleID = '<stdin>'
target datalayout = "e-p:64:64:64-i1:8:8-i8:8:8-i16:16:16-i32:32:32-i64:64:64-f32:32:32-f64:64:64-v64:64:64-v128:128:128-a0:0:64-s0:64:64-f80:128:128-n8:16:32:64-S128"
target triple = "x86_64-redhat-linux-gnu"

@v = global float 0.000000e+00
@a = global i32 0

define float @fortest() {
entry:
  %i = alloca i32
  %sum = alloca float
  %sum1 = load float* %sum
  %v = load float* @v
  store float %v, float* %sum
  %i2 = load i32* %i
  store i32 0, i32* %i
  br label %0

; <label>:0                                       ; preds = %4, %entry
  br i1 %2, label %1, label %5

; <label>:1                                       ; preds = %0
  %i3 = load i32* %i
  %a = load i32* @a
  %2 = icmp slt i32 %i3, %a
  %i4 = load i32* %i
  %3 = icmp sgt i32 %i4, 5
  br i1 %3, label %6, label %7
                                                  ; No predecessors!
  %i6 = load i32* %i
  br label %0

; <label>:5                                       ; preds = %6, %0
  %sum7 = load float* %sum
  ret float %sum7

; <label>:6                                       ; preds = %1
  br label %5

; <label>:7                                       ; preds = %1
  %sum5 = load float* %sum
  %8 = fmul float %sum5, 3.000000e+00
  store float %8, float* %sum
}
